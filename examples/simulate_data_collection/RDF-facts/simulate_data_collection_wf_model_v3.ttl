#####  Entity  #####

<simulate_data_collection#cassette_id_entity>
    rdf:type                    prov:Entity ;
    rdfs:label                  "cassette_id" ;
    rdfs:comment                "The unique ID of the cassette containing the crystals." .

<simulate_data_collection#sample_score_cutoff_entity>
    rdf:type                    prov:Entity ;
    rdfs:label                  "sample_score_cutoff" ;
    rdfs:comment                "The minimum quality score required of crystals." .

<simulate_data_collection#data_redundancy_entity>
    rdf:type                    prov:Entity ;
    rdfs:label                  "data_redundancy" ;
    rdfs:comment                "The desired redundancy of the data sets collected." .

<simulate_data_collection#sample_spreadsheet_entity>
    rdf:type                    p1:Data ;
    rdf:type                    prov:Entity ;
    rdf:type                    prov:DirectQueryService ;
    rdfs:label                  "sample_spreadsheet" ;
    rdfs:comment                "CSV file giving quality score for each crystal." ;
    prov:provenanceUriTemplate  "file:cassette_{cassette_id}_spreadsheet.csv" ;
    prov:wasDerivedFrom         <simulate_data_collection#cassette_id_entity> .

<simulate_data_collection#calibration_image_entity>
    rdf:type                    p1:Data ;
    rdf:type                    prov:Entity ;
    rdf:type                    prov:DirectQueryService ;
    rdfs:label                  "calibration_image" ;
    rdfs:comment                "File used to correct raw diffraction images." ;
    prov:provenanceUriTemplate  "file:calibration.img" .

<simulate_data_collection#run_log_entity>
    rdf:type                    p1:Data ;
    rdf:type                    prov:Entity ;
    rdf:type                    prov:DirectQueryService ;
    rdfs:label                  "run_log" ;
    rdfs:comment                "" ;
    prov:provenanceUriTemplate  "file:run/run_log.txt" .

<simulate_data_collection#sample_name_entity>
    rdf:type                    prov:Entity ;
    rdfs:label                  "sample_name" ;
    rdfs:comment                "" .

<simulate_data_collection#sample_quality_entity>
    rdf:type                    prov:Entity ;
    rdfs:label                  "sample_quality" ;
    rdfs:comment                "" .

<simulate_data_collection#rejected_sample_entity>
    rdf:type                    prov:Entity ;
    rdfs:label                  "rejected_sample" ;
    rdfs:comment                "" .

<simulate_data_collection#accepted_sample_entity>
    rdf:type                    prov:Entity ;
    rdfs:label                  "accepted_sample" ;
    rdfs:comment                "" .

<simulate_data_collection#num_images_entity>
    rdf:type                    prov:Entity ;
    rdfs:label                  "num_images" ;
    rdfs:comment                "" .

<simulate_data_collection#energies_entity>
    rdf:type                    prov:Entity ;
    rdfs:label                  "energies" ;
    rdfs:comment                "" .

<simulate_data_collection#rejection_log_entity>
    rdf:type                    p1:Data ;
    rdf:type                    prov:Entity ;
    rdf:type                    prov:DirectQueryService ;
    rdfs:label                  "rejection_log" ;
    rdfs:comment                "" ;
    prov:provenanceUriTemplate  "file:run/rejected_samples.txt" .

<simulate_data_collection#frame_number_entity>
    rdf:type                    prov:Entity ;
    rdfs:label                  "frame_number" ;
    rdfs:comment                "Index of diffraction image within data set." .

<simulate_data_collection#sample_id_entity>
    rdf:type                    prov:Entity ;
    rdfs:label                  "sample_id" ;
    rdfs:comment                "The crystal that the diffraction image was collected from." .

<simulate_data_collection#energy_entity>
    rdf:type                    prov:Entity ;
    rdfs:label                  "energy" ;
    rdfs:comment                "Energy (in eV) at which the diffraction image was collected." .

<simulate_data_collection#raw_image_entity>
    rdf:type                    p1:Data ;
    rdf:type                    prov:Entity ;
    rdf:type                    prov:DirectQueryService ;
    rdfs:label                  "raw_image" ;
    rdfs:comment                "Path of file storing the raw diffraction image." ;
    prov:provenanceUriTemplate  "file:run/raw/{cassette_id}/{sample_id}/e{energy}/image_{frame_number}.raw" ;
    prov:wasDerivedFrom         <simulate_data_collection#cassette_id_entity> ,
                                <simulate_data_collection#sample_id_entity> ,
                                <simulate_data_collection#energy_entity> ,
                                <simulate_data_collection#frame_number_entity> .

<simulate_data_collection#corrected_image_entity>
    rdf:type                    p1:Data ;
    rdf:type                    prov:Entity ;
    rdf:type                    prov:DirectQueryService ;
    rdfs:label                  "corrected_image" ;
    rdfs:comment                "The corrected diffraction images collected on all crystals." ;
    prov:provenanceUriTemplate  "file:run/data/{sample_id}/{sample_id}_{energy}eV_{frame_number}.img" ;
    prov:wasDerivedFrom         <simulate_data_collection#sample_id_entity> ,
                                <simulate_data_collection#energy_entity> ,
                                <simulate_data_collection#frame_number_entity> .

<simulate_data_collection#total_intensity_entity>
    rdf:type                    prov:Entity ;
    rdfs:label                  "total_intensity" ;
    rdfs:comment                "" .

<simulate_data_collection#pixel_count_entity>
    rdf:type                    prov:Entity ;
    rdfs:label                  "pixel_count" ;
    rdfs:comment                "" .

<simulate_data_collection#collection_log_entity>
    rdf:type                    p1:Data ;
    rdf:type                    prov:Entity ;
    rdf:type                    prov:DirectQueryService ;
    rdfs:label                  "collection_log" ;
    rdfs:comment                "" ;
    prov:provenanceUriTemplate  "file:run/collected_images.csv" .

#####################################################################################################################################
#      simulate_data_collection                                                                                                     #
#####################################################################################################################################
<simulate_data_collection>
    rdf:type            p1:Workflow ;
    rdfs:label          "simulate_data_collection" ;
    rdfs:comment        "Workflow for collecting diffraction data from high quality crystals in a cassette." ;
    p1:hasInPort        <simulate_data_collection#cassette_id_port> ,
                        <simulate_data_collection#sample_score_cutoff_port> ,
                        <simulate_data_collection#sample_spreadsheet_port> ,
						<simulate_data_collection#data_redundancy_port> ,
                        <simulate_data_collection#calibration_image_port> ;
    p1:hasOutPort       <simulate_data_collection#corrected_image_port> ,
                        <simulate_data_collection#run_log_port> ,
                        <simulate_data_collection#collection_log_port> ,
                        <simulate_data_collection#rejection_log_port> ;
    p1:hasSubProgram    <simulate_data_collection/initialize_run> ,
                        <simulate_data_collection/load_screening_results> ,
                        <simulate_data_collection/calculate_strategy> ,
                        <simulate_data_collection/log_rejected_sample> ,
                        <simulate_data_collection/collect_data_set> ,
                        <simulate_data_collection/transform_images> ,
                        <simulate_data_collection/log_average_image_intensity>.

#####  Port  #####

<simulate_data_collection#cassette_id_port>
    rdf:type                    p1:Port ;
    rdfs:label                  "cassette_id" ;
    prov:wasAttributedTo        <simulate_data_collection/parameters> ;
    p1:hasDefaultParam          <simulate_data_collection#cassette_id_entity> ;
    p1:connectsTo               <simulate_data_collection#cassette_id_channel> .

<simulate_data_collection#sample_score_cutoff_port>
    rdf:type                    p1:Port ;
    rdfs:label                  "sample_score_cutoff" ;
    prov:wasAttributedTo        <simulate_data_collection/parameters> ;
    p1:hasDefaultParam          <simulate_data_collection#sample_score_cutoff_entity> ;
    p1:connectsTo               <simulate_data_collection#sample_score_cutoff_channel> .

<simulate_data_collection#sample_spreadsheet_port>
    rdf:type                    p1:Port ;
    rdfs:label                  "sample_spreadsheet" ;
    p1:hasDefaultParam          <simulate_data_collection#sample_spreadsheet_entity> ;
    p1:connectsTo               <simulate_data_collection#sample_spreadsheet_channel> .

<simulate_data_collection#data_redundancy_port>
	rdf:type                    p1:Port ;
    rdfs:label                  "data_redundancy" ;
    prov:wasAttributedTo        <simulate_data_collection/parameters> ;
    p1:hasDefaultParam          <simulate_data_collection#data_redundancy_entity> ;
    p1:connectsTo               <simulate_data_collection#data_redundancy_channel> .

<simulate_data_collection#calibration_image_port>
    rdf:type                    p1:Port ;
    rdfs:label                  "calibration_image" ;
    p1:hasDefaultParam          <simulate_data_collection#calibration_image_entity> ;
    p1:connectsTo               <simulate_data_collection#calibration_image_channel> .

<simulate_data_collection#corrected_image_port>
    rdf:type                    p1:Port ;
    rdfs:label                  "corrected_image" ;
    p1:hasDefaultParam          <simulate_data_collection#corrected_image_entity> ;
    p1:connectsTo               <simulate_data_collection#corrected_image_channel> .

<simulate_data_collection#run_log_port>
    rdf:type                    p1:Port ;
    rdfs:label                  "run_log" ;
    p1:hasDefaultParam          <simulate_data_collection#run_log_entity> ;
    p1:connectsTo               <simulate_data_collection#run_log_channel> .

<simulate_data_collection#collection_log_port>
    rdf:type                    p1:Port ;
    rdfs:label                  "collection_log" ;
    p1:hasDefaultParam          <simulate_data_collection#collection_log_entity> ;
    p1:connectsTo               <simulate_data_collection#collection_log_channel> .

<simulate_data_collection#rejection_log_port>
    rdf:type                    p1:Port ;
    rdfs:label                  "rejection_log" ;
    p1:hasDefaultParam          <simulate_data_collection#rejection_log_entity> ;
    p1:connectsTo               <simulate_data_collection#rejection_log_channel> .

